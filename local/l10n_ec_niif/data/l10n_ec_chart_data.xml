<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ec_chart_template" model="account.chart.template">
        <field name="name">Ecuador - Chart of Accounts based on NIIF</field>
        <field name="currency_id" ref="base.USD" />
        <field name="bank_account_code_prefix">10101</field>
        <field name="cash_account_code_prefix">10101</field>
        <field name="transfer_account_code_prefix">10101</field>
        <field name="code_digits" eval="4" />
        <field name="use_anglo_saxon" eval="True" />
    </record>
</odoo>
